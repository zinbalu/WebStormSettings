<keymap version="1" name="Default copy" parent="$default">
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="shift ctrl g" />
  </action>
  <action id="MethodHierarchy" />
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="alt button2" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="shift ctrl h" />
  </action>
  <action id="SavaAs" />
  <action id="Stop">
    <keyboard-shortcut first-keystroke="ctrl f2" />
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
</keymap>